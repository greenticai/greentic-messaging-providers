{
  "name": "messaging-telegram",
  "version": "0.4.11",
  "description": "Provider-core Telegram messaging provider pack.",
  "kind": "application",
  "publisher": "Greentic",
  "config_schema": {
    "provider_config": {
      "format": "json",
      "path": "schemas/messaging/telegram/config.schema.json"
    }
  },
  "components": [
    "ai.greentic.component-templates",
    "messaging-provider-telegram",
    "messaging-ingress-telegram",
    "templates"
  ],
  "extensions": {
    "greentic.provider-extension.v1": {
      "kind": "greentic.provider-extension.v1",
      "version": "0.4.11",
      "inline": {
        "providers": [
          {
            "provider_type": "messaging.telegram.bot",
            "capabilities": [
              "messaging"
            ],
            "ops": [
              "send",
              "reply"
            ],
            "config_schema_ref": "schemas/messaging/telegram/config.schema.json",
            "state_schema_ref": "",
            "runtime": {
              "component_ref": "messaging-provider-telegram",
              "export": "schema-core-api",
              "world": "greentic:provider/schema-core@1.0.0"
            },
            "docs_ref": ""
          }
        ]
      }
    },
    "messaging.provider_flow_hints": {
      "kind": "messaging.provider_flow_hints",
      "version": "0.4.11",
      "inline": {
        "messaging.telegram.bot": {
          "diagnostics": "diagnostics",
          "setup_custom": "setup_custom",
          "setup_default": "setup_default",
          "verify_webhooks": "verify_webhooks"
        }
      }
    },
    "messaging.provider_ingress.v1": {
      "kind": "messaging.provider_ingress.v1",
      "version": "0.4.11",
      "inline": {
        "component_ref": "messaging-ingress-telegram",
        "export": "handle-webhook",
        "world": "greentic:messaging-ingress-telegram/messaging-ingress-telegram@0.0.1",
        "capabilities": {
          "supports_webhook_validation": true,
          "content_types": [
            "application/json"
          ]
        }
      }
    }
  },
  "secret_requirements": [
    {
      "name": "TELEGRAM_BOT_TOKEN",
      "scope": "tenant",
      "description": "Telegram bot token used for sendMessage requests."
    }
  ],
  "flows": [
    {
      "id": "diagnostics",
      "file": "flows/diagnostics.ygtc",
      "tags": [
        "default"
      ],
      "entrypoints": [
        "diagnostics"
      ]
    },
    {
      "id": "setup_custom",
      "file": "flows/setup_custom.ygtc",
      "tags": [
        "default"
      ],
      "entrypoints": [
        "setup"
      ]
    },
    {
      "id": "setup_default",
      "file": "flows/setup_default.ygtc",
      "tags": [
        "default"
      ],
      "entrypoints": [
        "setup"
      ]
    },
    {
      "id": "verify_webhooks",
      "file": "flows/verify_webhooks.ygtc",
      "tags": [
        "maintenance"
      ],
      "entrypoints": [
        "diagnostics"
      ]
    }
  ],
  "component_sources": [
    {
      "id": "ai.greentic.component-templates",
      "version": "0.4.11",
      "world": "root:component/root",
      "supports": [
        "messaging"
      ],
      "profiles": {
        "default": "default",
        "supported": [
          "default"
        ]
      },
      "capabilities": {
        "wasi": {
          "random": false,
          "clocks": false
        },
        "host": {}
      },
      "wasm": "components/ai.greentic.component-templates.wasm"
    },
    {
      "id": "messaging-provider-telegram",
      "version": "0.4.11",
      "world": "greentic:provider/schema-core@1.0.0",
      "supports": [
        "messaging"
      ],
      "profiles": {
        "default": "default",
        "supported": [
          "default"
        ]
      },
      "capabilities": {
        "wasi": {
          "random": false,
          "clocks": false
        },
        "host": {}
      },
      "wasm": "components/messaging-provider-telegram.wasm"
    },
    {
      "id": "messaging-ingress-telegram",
      "version": "0.4.11",
      "world": "greentic:messaging-ingress-telegram/messaging-ingress-telegram@0.0.1",
      "supports": [
        "messaging"
      ],
      "profiles": {
        "default": "default",
        "supported": [
          "default"
        ]
      },
      "capabilities": {
        "wasi": {
          "random": false,
          "clocks": false
        },
        "host": {}
      },
      "wasm": "components/messaging-ingress-telegram.wasm"
    },
    {
      "id": "templates",
      "version": "0.4.11",
      "world": "greentic:component/component@0.5.0",
      "supports": [
        "messaging"
      ],
      "profiles": {
        "default": "stateless",
        "supported": [
          "stateless"
        ]
      },
      "capabilities": {
        "wasi": {
          "random": false,
          "clocks": false
        },
        "host": {}
      },
      "wasm": "components/templates.wasm"
    }
  ],
  "capabilities_cache": [
    {
      "component": "ai.greentic.component-templates",
      "version": "v1",
      "path": "components/ai.greentic.component-templates-capabilities_v1.json"
    },
    {
      "component": "templates",
      "version": "v1",
      "path": "components/templates-capabilities_v1.json"
    }
  ]
}
