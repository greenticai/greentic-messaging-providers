{
  "name": "messaging-email",
  "version": "0.4.12",
  "description": "Provider-core SMTP email sender pack (simulated send).",
  "kind": "application",
  "publisher": "Greentic",
  "config_schema": {
    "provider_config": {
      "format": "json",
      "path": "schemas/messaging/email/config.schema.json"
    }
  },
  "components": [
    "ai.greentic.component-templates",
    "messaging-provider-email",
    "templates",
    "handlebars"
  ],
  "extensions": {
    "greentic.provider-extension.v1": {
      "kind": "greentic.provider-extension.v1",
      "version": "0.4.12",
      "inline": {
        "providers": [
          {
            "provider_type": "messaging.email.smtp",
            "capabilities": [
              "messaging"
            ],
            "ops": [
              "send",
              "reply"
            ],
            "config_schema_ref": "schemas/messaging/email/config.schema.json",
            "state_schema_ref": "",
            "runtime": {
              "component_ref": "messaging-provider-email",
              "export": "schema-core-api",
              "world": "greentic:provider/schema-core@1.0.0"
            },
            "docs_ref": ""
          }
        ]
      }
    },
    "messaging.provider_flow_hints": {
      "kind": "messaging.provider_flow_hints",
      "version": "0.4.12",
      "inline": {
        "messaging.email.smtp": {
          "diagnostics": "diagnostics",
          "setup_custom": "setup_custom",
          "setup_default": "setup_default"
        }
      }
    }
  },
  "secret_requirements": [
    {
      "name": "EMAIL_PASSWORD",
      "scope": "tenant",
      "description": "SMTP password secret key"
    }
  ],
  "flows": [
    {
      "id": "diagnostics",
      "file": "flows/diagnostics.ygtc",
      "tags": [
        "default"
      ],
      "entrypoints": [
        "diagnostics"
      ]
    },
    {
      "id": "setup_custom",
      "file": "flows/setup_custom.ygtc",
      "tags": [
        "default"
      ],
      "entrypoints": [
        "setup"
      ]
    },
    {
      "id": "setup_default",
      "file": "flows/setup_default.ygtc",
      "tags": [
        "default"
      ],
      "entrypoints": [
        "setup"
      ]
    },
    {
      "id": "requirements",
      "file": "flows/requirements.ygtc",
      "tags": [
        "default"
      ],
      "entrypoints": [
        "requirements"
      ]
    }
  ],
  "component_sources": [
    {
      "id": "ai.greentic.component-templates",
      "version": "0.4.12",
      "world": "root:component/root",
      "supports": [
        "messaging"
      ],
      "profiles": {
        "default": "default",
        "supported": [
          "default"
        ]
      },
      "capabilities": {
        "wasi": {
          "random": false,
          "clocks": false
        },
        "host": {}
      },
      "wasm": "components/ai.greentic.component-templates.wasm"
    },
    {
      "id": "messaging-provider-email",
      "version": "0.4.12",
      "world": "greentic:provider/schema-core@1.0.0",
      "supports": [
        "messaging"
      ],
      "profiles": {
        "default": "default",
        "supported": [
          "default"
        ]
      },
      "capabilities": {
        "wasi": {
          "random": false,
          "clocks": false
        },
        "host": {}
      },
      "wasm": "components/messaging-provider-email.wasm"
    },
    {
      "id": "templates",
      "version": "0.4.12",
      "world": "greentic:component/component@0.5.0",
      "supports": [
        "messaging"
      ],
      "profiles": {
        "default": "stateless",
        "supported": [
          "stateless"
        ]
      },
      "capabilities": {
        "wasi": {
          "random": false,
          "clocks": false
        },
        "host": {}
      },
      "wasm": "components/templates.wasm"
    },
    {
      "id": "handlebars",
      "version": "0.4.12",
      "world": "greentic:component/component@0.5.0",
      "supports": [
        "messaging"
      ],
      "profiles": {
        "default": "stateless",
        "supported": [
          "stateless"
        ]
      },
      "capabilities": {
        "wasi": {
          "random": false,
          "clocks": false
        },
        "host": {}
      },
      "wasm": "components/templates.wasm"
    }
  ],
  "capabilities_cache": [
    {
      "component": "ai.greentic.component-templates",
      "version": "v1",
      "path": "components/ai.greentic.component-templates-capabilities_v1.json"
    },
    {
      "component": "templates",
      "version": "v1",
      "path": "components/templates-capabilities_v1.json"
    }
  ]
}
