      "tags": [
        "default"
      ],
      "entrypoints": [
        "setup"
      ]
    },
    {
      "id": "verify_webhooks",
      "file": "flows/verify_webhooks.ygtc",
      "tags": [
        "maintenance"
      ],
      "entrypoints": [
        "diagnostics"
      ]
    }
  ],
