{
  "name": "messaging-whatsapp",
  "version": "0.4.16",
  "description": "Provider-core WhatsApp Cloud messaging pack.",
  "kind": "application",
  "publisher": "Greentic",
  "config_schema": {
    "provider_config": {
      "format": "json",
      "path": "schemas/messaging/whatsapp/config.schema.json"
    }
  },
  "components": [
    "ai.greentic.component-templates",
    "messaging-ingress-whatsapp",
    "messaging-provider-whatsapp"
  ],
  "extensions": {
    "greentic.provider-extension.v1": {
      "kind": "greentic.provider-extension.v1",
      "version": "0.4.15",
      "inline": {
        "providers": [
          {
            "provider_type": "messaging.whatsapp.cloud",
            "capabilities": [
              "messaging"
            ],
            "ops": [
              "send",
              "reply"
            ],
            "config_schema_ref": "schemas/messaging/whatsapp/config.schema.json",
            "state_schema_ref": "",
            "runtime": {
              "component_ref": "messaging-provider-whatsapp",
              "export": "schema-core-api",
              "world": "greentic:provider/schema-core@1.0.0"
            },
            "docs_ref": ""
          }
        ]
      }
    },
    "messaging.provider_flow_hints": {
      "kind": "messaging.provider_flow_hints",
      "version": "0.4.15",
      "inline": {
        "messaging.whatsapp.cloud": {
          "diagnostics": "diagnostics",
          "setup_custom": "setup_custom",
          "setup_default": "setup_default",
          "verify_webhooks": "verify_webhooks"
        }
      }
    },
    "messaging.provider_ingress.v1": {
      "kind": "messaging.provider_ingress.v1",
      "version": "0.4.15",
      "inline": {
        "capabilities": {
          "content_types": [
            "application/json"
          ],
          "supports_webhook_validation": true
        },
        "component_ref": "messaging-ingress-whatsapp",
        "export": "handle-webhook",
        "world": "greentic:messaging-ingress-whatsapp/messaging-ingress-whatsapp@0.0.1"
      }
    }
  },
  "secret_requirements": [
    {
      "name": "WHATSAPP_VERIFY_TOKEN",
      "scope": "tenant",
      "description": "Verify token used for WhatsApp webhook validation (if configured)."
    },
    {
      "name": "WHATSAPP_TOKEN",
      "scope": "tenant",
      "description": "WhatsApp Cloud API access token."
    }
  ],
  "flows": [
    {
      "id": "diagnostics",
      "file": "flows/diagnostics.ygtc",
      "tags": [
        "default"
      ],
      "entrypoints": [
        "diagnostics"
      ]
    },
    {
      "id": "requirements",
      "file": "flows/requirements.ygtc",
      "tags": [
        "default"
      ],
      "entrypoints": [
        "requirements"
      ]
    },
    {
      "id": "setup_custom",
      "file": "flows/setup_custom.ygtc",
      "tags": [
        "default"
      ],
      "entrypoints": [
        "setup"
      ]
    },
    {
      "id": "setup_default",
      "file": "flows/setup_default.ygtc",
      "tags": [
        "default"
      ],
      "entrypoints": [
        "setup"
      ]
    },
    {
      "id": "verify_webhooks",
      "file": "flows/verify_webhooks.ygtc",
      "tags": [
        "maintenance"
      ],
      "entrypoints": [
        "diagnostics"
      ]
    }
  ],
